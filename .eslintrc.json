{
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 2021
	},

	"env": {
		"es6": true,
		"node": true
	},

	"extends": [
		"eslint:recommended",
		"google"
	],

	"rules": {
		"semi": ["error", "never"],
		"require-jsdoc": "off",
		"indent": ["error", "tab"],
		"no-tabs": "off",
		"comma-dangle": ["error", "never"],
		"arrow-parens": "off"
	}
}